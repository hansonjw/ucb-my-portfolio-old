og(this.state.message);\n    }\n\n    render() {\n        return(\n            <div class=\"container form-contact\">\n                <form onSubmit={this.handleSubmit}>\n                    <div class=\"form-group\">\n                        <label for=\"nameInput\">Name:</label>\n                        <input type=\"text\" class=\"form-control\" name=\"name\" id=\"nameInput\" value={this.state.name} onChange={this.handleChange} />\n                    </div>\n                    <div class=\"form-group\">\n                        <label for=\"emailInput\">Email:</label>\n                        <input type=\"email\" class=\"form-control\" name=\"email\" id='emailInput' value={this.state.email} onChange={this.handleChange} />\n                    </div>\n                    <div class=\"form-group\">\n                        <label for=\"messageInput\">Message:</label>\n                        <textarea name=\"message\" class=\"form-control\" rows=\"5\" id=\"messageInput\" value={this.state.message} onChange={this.handleChange} />\n                    </div>\n                    <div class=\"form-group py-3 btn-contact-container\">\n                        <input type=\"submit\" class=\"btn-contact\" />\n                    </div>\n                </form>\n            </div>\n        )\n\n    }\n}\n\nexport default Contact;","/Users/justinhanson/prepos/my-portfolio/src/components/Resume/index.js",["57","58","59"],"import React from 'react';\nimport resume from \"../../assets/resume.pdf\";\nimport resume1 from \"../../assets/resume1.png\";\nimport resume2 from \"../../assets/resume2.png\";\nimport iconDownload from \"../../assets/iconDownload.svg\";\n\nfunction Resume() {\n\n    return(\n        <div class=\"container\">\n            <div class=\"row px-auto\">\n                <p class=\"p-resume text-center\">\n                <a class=\"links\" href={resume} download='resume-Justin-Hanson'>Please click here to download a pdf copy of my resume.  Thank you for vi